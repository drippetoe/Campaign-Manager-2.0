alias makeAlias="vi ~/.aliases"
alias sourceAlias="source ~/.aliases"
alias bt='/usr/sbin/hciconfig -a'
alias btoff='/home/proximus/bin/bash_scripts/bluetooth.sh stop'
alias bton='/home/proximus/bin/bash_scripts/bluetooth.sh start'
alias btname='/home/proximus/bin/bash_scripts/bluetooth.sh name'
alias ssid='grep ssid /etc/hostapd.conf |grep -v _ssid'
alias channel='grep channel /etc/hostapd.conf'
alias home="cd /home/proximus/"
alias start="/home/proximus/bin/start_proximus_client.sh &"
alias stop="/home/proximus/bin/bash_scripts/kill_proximus.sh"
alias proxstart="/home/proximus/bin/start_proximus_client.sh &"
alias proxstop="/home/proximus/bin/bash_scripts/kill_proximus.sh"
alias psprox='ps w | grep DENVERCODER | grep -v grep'
alias proxps="ps w | grep DENVERCODER | grep -v grep"
alias proxlog="tail -f /home/proximus/logs/client/proximus.log"
alias mkdirs="mkdir -p"
alias override="touch /home/proximus/bin/override"
alias roverride="rm -f /home/proximus/bin/override"
alias bin="cd /home/proximus/bin"
alias logs="cd /home/proximus/logs"
alias config="cd /home/proximus/config"
alias restart="/home/proximus/bin/bash_scripts/restart.sh"
alias tmain="tail -f /home/proximus/logs/client/proximus.log"
alias tweb="tail -f $(grep '^accesslog.filename' /home/proximus/config/lighttpd-inc.conf | awk '{print $3}')"
alias tbt="tail -f $(grep '^log ' /home/proximus/config/obexsender.conf  | awk '{print $2}')"
alias cme="grep ERROR /home/proximus/logs/client/proximus.log"
alias cmi="grep INFO /home/proximus/logs/client/proximus.log"
alias cmd="grep DEBUG /home/proximus/logs/client/proximus.log"
alias campStatus="cat /home/proximus/config/campaigns.cfg.xml"
alias statuslighttpd="service lighttpd status"
